{
  "meta": {
    "name": "Drift-Based Rebalancing (5% threshold)",
    "notes": "Three-asset portfolio (SPY/AGG/TLT) that rebalances when any position drifts more than 5% from target. Taxable account with tax-aware trading."
  },
  "universe": {
    "type": "CUSTOM",
    "symbols": ["SPY", "AGG", "TLT"]
  },
  "period": {
    "start": "2015-01-01",
    "end": "2024-12-31",
    "calendar": "NYSE"
  },
  "initial_cash": 100000,
  "account": {
    "type": "Taxable",
    "tax": {
      "federal_ordinary": 0.32,
      "federal_ltcg": 0.15,
      "state": 0.06,
      "qualified_dividend_pct": 0.8,
      "apply_wash_sale": true,
      "pay_taxes_from_external": false,
      "withdrawal_tax_rate_for_ira": 0.25
    },
    "contribution_caps": {
      "enforce": false,
      "ira": 7000,
      "ira_catch_up": 1000,
      "roth": 7000,
      "roth_catch_up": 1000
    }
  },
  "dividends": {
    "mode": "DRIP",
    "reinvest_threshold_pct": 0.0
  },
  "rebalancing": {
    "type": "drift",
    "drift": {
      "abs_pct": 0.05,
      "rel_pct": null
    }
  },
  "orders": {
    "timing": "MOO"
  },
  "lots": {
    "method": "HIFO"
  },
  "frictions": {
    "commission_per_trade": 0.0,
    "slippage_bps": 5.0,
    "use_actual_etf_er": true,
    "equity_borrow_bps": 0.0
  },
  "signals": [],
  "rules": {
    "entry": [],
    "exit": []
  },
  "position_sizing": {
    "method": "EQUAL_WEIGHT"
  },
  "benchmark": ["SPY"],
  "exports": {
    "csv": true,
    "json": true,
    "pdf": false
  }
}
